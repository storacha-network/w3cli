{
  "name": "@web3-storage/w3cli",
  "type": "module",
  "version": "4.6.0",
  "license": "(Apache-2.0 AND MIT)",
  "description": "ðŸ’¾ w3 command line interface",
  "bin": {
    "w3": "shim.js",
    "w3up": "shim.js"
  },
  "scripts": {
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "eslint --fix '**/*.{js,ts}'",
    "check": "tsc --build",
    "format": "prettier --write '**/*.{js,ts,yml,json}' --ignore-path .gitignore",
    "test": "npm-run-all -p test:ava",
    "test:ava": "ava --verbose --serial --timeout=1m",
    "test:watch": "ava --serial --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3-storage/w3cli.git"
  },
  "keywords": [
    "w3",
    "web3",
    "storage",
    "upload",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/web3-storage/w3cli/issues"
  },
  "homepage": "https://github.com/web3-storage/w3cli#readme",
  "devDependencies": {
    "@types/update-notifier": "^6.0.5",
    "@ucanto/principal": "^9.0.0",
    "@ucanto/server": "^9.0.1",
    "@ucanto/interface": "^9.0.0",
    "@web3-storage/capabilities": "^10.2.0",
    "@web3-storage/upload-client": "^11.1.0",
    "@web3-storage/upload-api": "^7.1.0",
    "@web3-storage/sigv4": "^1.0.2",
    "@web-std/blob": "^3.0.5",
    "ava": "^5.1.0",
    "execa": "^8.0.1",
    "multiformats": "^12.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3",
    "@web3-storage/eslint-config-w3up": "^1.0.0"
  },
  "dependencies": {
    "@ipld/car": "^5.2.4",
    "@ipld/dag-json": "^10.1.5",
    "@ipld/dag-ucan": "^3.4.0",
    "@ucanto/client": "^9.0.0",
    "@ucanto/core": "^9.0.0",
    "@ucanto/transport": "^9.0.0",
    "@web3-storage/access": "^16.4.0",
    "@web3-storage/did-mailto": "^2.0.1",
    "@web3-storage/w3up-client": "^9.2.0",
    "chalk": "^5.3.0",
    "files-from-path": "^1.0.0",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "pretty-tree": "^1.0.0",
    "sade": "^1.8.1",
    "update-notifier": "^6.0.2"
  },
  "eslintConfig": {
    "extends": [
      "@web3-storage/eslint-config-w3up"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "es2022": true,
      "mocha": true,
      "browser": true,
      "node": true
    },
    "ignorePatterns": [
      "dist",
      "coverage",
      "api.js"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
